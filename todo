sudo systemd-nspawn -M ocitysmap --chdir=/root gs -o./map.pdf -c '[ /Rect [0 425 295 465] /Border [0 0 2] /Color [.7 0 0] /Page 1 /Subtype /Link /ANN pdfmark' -f ./ocitysmap/citymap.pdf -sDEVICE=pdfwrite && sudo cp /var/lib/machines/ocitysmap/root/map.pdf ~/Downloads/map2.pdf

